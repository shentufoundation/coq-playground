{
  "version": 2,
  "routes": [
    {
      "src": "/(.*\\.(js|css|png|svg|cjs|gif))",
      "dest": "/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/jscoq/(coq-pkgs|docs)/(.*)",
      "dest": "/jscoq/$1/$2",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/",
      "dest": "/scratchpad.html"
    },
    {
      "src": "/scratchpad.html",
      "dest": "/scratchpad.html"
    },
    {
      "src": "/(.*)",
      "status": 404
    }
  ]
}

